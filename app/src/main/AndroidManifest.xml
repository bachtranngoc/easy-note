<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.bkap.easynote.activities" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_note"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddNoteAcitivity"
            android:label="@string/title_activity_add_note_acitivity"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".CustomNoteAcitivity"
            android:label="@string/title_activity_fix_note_acitivity"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".CustomActivity"
            android:label="@string/title_activity_custom"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
